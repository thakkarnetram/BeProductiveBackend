{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "cross-env $(cat .env.dev | xargs) nodemon server.js",
    "dev": "cross-env $(cat .env.dev | xargs) nodemon server.js",
    "test": "cross-env $(cat .env.test | xargs) jest --runInBand --detectOpenHandles --maxWorker=100%"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node",
      "ejs"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironmentOptions": {
      "PORT": 8083
    },
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testTimeout": 20000
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ejs": "^3.1.9",
    "env": "^0.0.2",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "jest-environment-node": "^29.7.0",
    "jest-runner": "^29.7.0",
    "jsonwebtoken": "^9.0.1",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.1.0",
    "mongoose": "^7.2.4",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "randomstring": "^1.3.0",
    "shortid": "^2.2.16",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
